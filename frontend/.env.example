# Frontend Environment Variables
# Copy this file to .env and update values as needed

# Backend API URL (used at build time for Vite)
VITE_BACKEND_API_URL=http://localhost:8000

# Chat API URL (optional)
VITE_API_URL=http://localhost:3001

# Better Auth URL (optional, defaults to VITE_BACKEND_API_URL)
VITE_BETTER_AUTH_URL=http://localhost:8000

# App Name
VITE_APP_NAME=Todo AI Chatbot

# Runtime config (used by Docker container at runtime)
# This is injected into config.js by nginx startup script
BACKEND_API_URL=http://localhost:8000
